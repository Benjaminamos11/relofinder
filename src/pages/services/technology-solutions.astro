---
import Layout from '../../layouts/Layout.astro';
import CompanyCard from '../../components/companies/CompanyCard.astro';
import { getCollection } from 'astro:content';

// Get the technology solutions content
const services = await getCollection('services');
const technologyService = services.find(service => service.slug === 'technology-solutions');

// Mock data for technology solutions if not found
const serviceData = technologyService?.data || {
  title: 'Technology Solutions for Swiss Relocation',
  description: 'Comprehensive digital solutions for modern relocations. Virtual tours, digital document management, online integration platforms, and smart relocation technology.',
  heroImage: 'https://images.unsplash.com/photo-1518709268805-4e9042af2176?w=1600&h=900&fit=crop',
  stats: {
    providers: '15+',
    avgTime: '1-3 months',
    satisfaction: '4.8/5'
  },
  process: [
    {
      step: '1',
      title: 'Digital Assessment',
      description: 'Evaluate your technology needs, current tools, and digital integration requirements for Swiss relocation.',
      duration: '1-2 hours'
    },
    {
      step: '2',
      title: 'Platform Setup',
      description: 'Configure digital relocation platform, document management systems, and collaboration tools.',
      duration: '3-5 days'
    },
    {
      step: '3',
      title: 'Virtual Discovery',
      description: 'Access virtual property tours, neighborhood exploration, and remote service consultations.',
      duration: '2-4 weeks'
    },
    {
