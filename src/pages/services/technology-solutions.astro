    description: 'Complete digital ecosystem for international relocations with blockchain document verification.',
    services: ['Digital Ecosystem', 'Blockchain Verification', 'Smart Contracts'],
    regions: ['All Swiss Regions'],
    rating: 4.7,
    reviewCount: 98,
    consultationUrl: 'https://digitalmove.ch/book',
    quoteUrl: 'https://digitalmove.ch/quote'
  },
  {
    id: 'virtual-relocation-pro',
    name: 'Virtual Relocation Pro',
    logo: 'https://images.pexels.com/photos/3184339/pexels-photo-3184339.jpeg?auto=compress&cs=tinysrgb&w=200',
    description: 'Advanced VR and AR solutions for immersive property viewing and neighborhood exploration.',
    services: ['VR Property Tours', 'AR Exploration', 'Remote Consultations'],
    regions: ['Zurich', 'Geneva', 'Zug'],
    rating: 4.8,
    reviewCount: 76,
    consultationUrl: 'https://virtualrelopro.ch/consultation',
    quoteUrl: 'https://virtualrelopro.ch/proposal'
  },
  {
    id: 'smart-integration-systems',
    name: 'Smart Integration Systems',
    logo: 'https://images.pexels.com/photos/8112165/pexels-photo-8112165.jpeg?auto=compress&cs=tinysrgb&w=200',
    description: 'IoT and smart home integration services for seamless technology transition in Swiss properties.',
    services: ['IoT Integration', 'Smart Home Setup', 'Tech Transition'],
    regions: ['Basel', 'Lausanne', 'Lucerne'],
    rating: 4.6,
    reviewCount: 89,
    consultationUrl: 'https://smartintegration.ch/book',
    quoteUrl: 'https://smartintegration.ch/quote'
  }
];

// Related services for internal linking
const relatedServices = [
  {
    title: 'Advisory Services',
    description: 'Strategic technology planning for digital-first relocations.',
    href: '/services/advisory-services',
    icon: 'lightbulb'
  },
  {
    title: 'Housing Services',
    description: 'Virtual property search and digital housing platforms.',
    href: '/services/housing',
    icon: 'home'
  },
  {
    title: 'Move Management',
    description: 'Digital tracking and smart logistics coordination.',
    href: '/services/move-management',
    icon: 'truck'
  },
  {
    title: 'Ongoing Support',
    description: 'Continuous digital platform support and tech assistance.',
    href: '/services/ongoing-support',
    icon: 'support'
  }
];

// Technology solution categories
const technologyCategories = [
  {
    category: 'Virtual Reality & Tours',
    href: '/services/technology-solutions#virtual',
    description: 'Immersive property viewing, neighborhood exploration, and remote consultation experiences.',
    features: ['360° Property Tours', 'VR Neighborhood Walks', 'Remote Consultations']
  },
  {
    category: 'Digital Platform Management',
    href: '/services/technology-solutions#platform',
    description: 'Comprehensive digital hubs for managing all aspects of your Swiss relocation journey.',
    features: ['Unified Dashboard', 'Progress Tracking', 'Communication Hub']
  },
  {
    category: 'Smart Document Systems',
    href: '/services/technology-solutions#documents',
    description: 'Secure digital document management with AI-powered processing and verification.',
    features: ['Digital Vault', 'AI Processing', 'Blockchain Verification']
  },
  {
    category: 'Integration & Automation',
    href: '/services/technology-solutions#automation',
    description: 'Smart systems that automate routine tasks and integrate with Swiss digital infrastructure.',
    features: ['Task Automation', 'System Integration', 'Smart Notifications']
  }
];

// Location-specific technology adoption
const locationTechnology = [
  {
    location: 'Zurich',
    href: '/regions/zurich#technology',
    description: 'Leading fintech and digital innovation hub with advanced smart city infrastructure.',
    techFocus: ['Fintech Integration', 'Smart City', 'Digital Banking']
  },
  {
    location: 'Geneva',
    href: '/regions/geneva#technology',
    description: 'International tech standards with diplomatic technology protocols and multilingual platforms.',
    techFocus: ['International Standards', 'Diplomatic Tech', 'Multilingual AI']
  },
  {
    location: 'Basel',
    href: '/regions/basel#technology',
    description: 'Pharmaceutical tech integration with cross-border digital systems and research platforms.',
    techFocus: ['Pharma Tech', 'Cross-Border Systems', 'Research Platforms']
  },
  {
    location: 'Zug',
    href: '/regions/zug',
    description: 'Crypto Valley innovation with blockchain solutions and entrepreneurial tech ecosystem.',
    techFocus: ['Blockchain Tech', 'Crypto Integration', 'Startup Ecosystem']
  }
];

// Blog resources
const blogResources = [
  {
    title: 'Digital Relocation: Future of Moving to Switzerland',
    href: '/blog/digital-relocation-future',
    category: 'Innovation',
    readTime: '12 min'
  },
  {
    title: 'Virtual Property Tours: Complete Guide 2025',
    href: '/blog/virtual-property-tours-guide',
    category: 'Technology',
    readTime: '8 min'
  },
  {
    title: 'Smart Home Integration in Switzerland',
    href: '/blog/smart-home-integration-switzerland',
    category: 'Smart Tech',
    readTime: '10 min'
  },
  {
    title: 'Digital Document Management for Expats',
    href: '/blog/digital-document-management-expats',
    category: 'Digital Tools',
    readTime: '15 min'
  }
];

// Page metadata
const title = 'Technology Solutions for Swiss Relocation | Digital & Smart Moving Services';
const description = 'Advanced technology solutions for Swiss relocations. Virtual tours, digital platforms, smart document management, and AI-powered relocation assistance.';
---

<Layout title={title} description={description}>
  <!-- Hero Section -->
  <section class="relative py-20 bg-gradient-to-br from-cyan-50 via-white to-blue-50 overflow-hidden">
    <div class="absolute inset-0 opacity-30">
      <div class="absolute inset-0 bg-repeat" style="background-image: url(data:image/svg+xml,%3Csvg%20width%3D%2260%22%20height%3D%2260%22%20viewBox%3D%220%200%2060%2060%22%20xmlns%3D%22http%3A//www.w3.org/2000/svg%22%3E%3Cg%20fill%3D%22none%22%20fill-rule%3D%22evenodd%22%3E%3Cg%20fill%3D%22%230891b2%22%20fill-opacity%3D%220.05%22%3E%3Ccircle%20cx%3D%2230%22%20cy%3D%2230%22%20r%3D%224%22/%3E%3C/g%3E%3C/g%3E%3C/svg%3E);"></div>
    </div>

    <div class="container relative z-10">
      <div class="max-w-6xl mx-auto">
        <div class="grid lg:grid-cols-2 gap-12 items-center">
          <!-- Content -->
          <div class="space-y-8">
            <!-- Breadcrumb -->
            <nav class="flex items-center space-x-2 text-sm text-gray-600">
              <a href="/" class="hover:text-primary-600 transition-colors">Home</a>
              <span>→</span>
              <a href="/services" class="hover:text-primary-600 transition-colors">Services</a>
              <span>→</span>
              <span class="text-gray-900">Technology Solutions</span>
            </nav>

            <!-- Service Badge -->
            <div class="inline-flex items-center px-4 py-2 rounded-full bg-cyan-100 text-cyan-700 font-medium text-sm">
              <svg class="w-4 h-4 mr-2" fill="currentColor" viewBox="0 0 20 20">
                <path fill-rule="evenodd" d="M3 4a1 1 0 011-1h12a1 1 0 011 1v2a1 1 0 01-1 1H4a1 1 0 01-1-1V4zm0 4a1 1 0 011-1h6a1 1 0 011 1v6a1 1 0 01-1 1H4a1 1 0 01-1-1V8zm8 0a1 1 0 011-1h4a1 1 0 011 1v2a1 1 0 01-1 1h-4a1 1 0 01-1-1V8zm0 4a1 1 0 011-1h4a1 1 0 011 1v2a1 1 0 01-1 1h-4a1 1 0 01-1-1v-2z" clip-rule="evenodd"/>
              </svg>
              Digital Innovation
            </div>

            <!-- Title & Description -->
            <div class="space-y-6">
              <h1 class="text-4xl lg:text-6xl font-bold leading-tight">
                Technology <span class="gradient-text">Solutions</span> for Swiss Relocation
              </h1>
              <p class="text-xl text-gray-600 leading-relaxed">
                {serviceData.description}
              </p>
            </div>

            <!-- Stats -->
            <div class="grid grid-cols-3 gap-6">
              <div class="text-center">
                <div class="text-3xl font-bold text-cyan-600">{serviceData.stats?.providers || '15+'}</div>
                <div class="text-sm text-gray-600">Tech Providers</div>
              </div>
              <div class="text-center">
                <div class="text-3xl font-bold text-cyan-600">{serviceData.stats?.avgTime || '1-3 months'}</div>
                <div class="text-sm text-gray-600">Implementation</div>
              </div>
              <div class="text-center">
                <div class="text-3xl font-bold text-cyan-600">{serviceData.stats?.satisfaction || '4.8/5'}</div>
                <div class="text-sm text-gray-600">User Rating</div>
              </div>
            </div>

            <!-- CTA -->
            <div class="flex flex-wrap gap-4">
                              <button onclick="openModal({page: 'service', service: 'Technology Solutions'})" class="btn-primary px-8 py-4">
                Get Tech Solutions
              </button>
              <a href="/swiss-relocation-guide#technology" class="btn-secondary px-8 py-4">
                Technology Guide
              </a>
            </div>
          </div>

          <!-- Hero Image -->
          <div class="relative">
            <img 
              src={serviceData.heroImage} 
              alt="Technology Solutions for Swiss Relocation" 
              class="rounded-3xl shadow-2xl"
              loading="eager"
            />
            <div class="absolute inset-0 rounded-3xl bg-gradient-to-tr from-cyan-600/20 to-transparent"></div>
          </div>
        </div>
      </div>
    </div>
  </section>

  <!-- Related Services Section -->
  <section class="py-16 bg-white">
    <div class="container">
      <div class="max-w-6xl mx-auto">
        <div class="text-center mb-12">
          <h2 class="text-3xl font-bold mb-4">Related Technology Services</h2>
          <p class="text-gray-600 max-w-2xl mx-auto">
            Technology solutions enhance and integrate with all aspects of your Swiss relocation
          </p>
        </div>

        <div class="grid md:grid-cols-2 lg:grid-cols-4 gap-6">
          {relatedServices.map((service) => (
            <a href={service.href} class="group bg-gradient-to-br from-gray-50 to-white rounded-2xl p-6 border border-gray-100 hover:shadow-lg transition-all duration-300 hover:-translate-y-1">
              <div class="w-12 h-12 bg-gradient-to-br from-primary-500 to-primary-600 rounded-xl flex items-center justify-center mb-4 group-hover:scale-110 transition-transform duration-300">
                {service.icon === 'lightbulb' && (
                  <svg class="w-6 h-6 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9.663 17h4.673M12 3v1m6.364 1.636l-.707.707M21 12h-1M4 12H3m3.343-5.657l-.707-.707m2.828 9.9a5 5 0 117.072 0l-.548.547A3.374 3.374 0 0014 18.469V19a2 2 0 11-4 0v-.531c0-.895-.356-1.754-.988-2.386l-.548-.547z"/>
                  </svg>
                )}
                {service.icon === 'home' && (
                  <svg class="w-6 h-6 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M3 12l2-2m0 0l7-7 7 7M5 10v10a1 1 0 001 1h3m10-11l2 2m-2-2v10a1 1 0 01-1 1h-3m-6 0a1 1 0 001-1v-4a1 1 0 011-1h2a1 1 0 011 1v4a1 1 0 001 1m-6 0h6"/>
                  </svg>
                )}
                {service.icon === 'truck' && (
                  <svg class="w-6 h-6 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 12l2 2 4-4m5.618-4.016A11.955 11.955 0 0112 2.944a11.955 11.955 0 01-8.618 3.04A12.02 12.02 0 003 9c0 5.591 3.824 10.29 9 11.622 5.176-1.332 9-6.03 9-11.622 0-1.042-.133-2.052-.382-3.016z"/>
                  </svg>
                )}
                {service.icon === 'support' && (
                  <svg class="w-6 h-6 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M18.364 5.636l-3.536 3.536m0 5.656l3.536 3.536M9.172 9.172L5.636 5.636m3.536 9.192L5.636 18.364M21 12a9 9 0 11-18 0 9 9 0 0118 0z"/>
                  </svg>
                )}
              </div>
              <h3 class="font-bold text-lg mb-2 group-hover:text-primary-600 transition-colors">{service.title}</h3>
              <p class="text-gray-600 text-sm">{service.description}</p>
            </a>
          ))}
        </div>
      </div>
    </div>
  </section>

  <!-- Technology Solution Categories -->
  <section class="py-16 bg-gray-50">
    <div class="container">
      <div class="max-w-6xl mx-auto">
        <div class="text-center mb-12">
          <h2 class="text-3xl font-bold mb-4">Technology <span class="gradient-text">Solution Categories</span></h2>
          <p class="text-gray-600 max-w-2xl mx-auto">
            Advanced digital solutions covering every aspect of modern Swiss relocations
          </p>
        </div>

        <div class="grid md:grid-cols-2 gap-8">
          {technologyCategories.map((category) => (
            <a href={category.href} class="bg-white rounded-2xl p-8 shadow-sm hover:shadow-lg transition-all duration-300 group hover:-translate-y-1">
              <h3 class="font-bold text-2xl mb-4 group-hover:text-primary-600 transition-colors">{category.category}</h3>
              <p class="text-gray-600 mb-6 leading-relaxed">{category.description}</p>
              <div class="space-y-3">
                <div class="text-sm font-medium text-gray-500">Key Features:</div>
                <div class="flex flex-wrap gap-2">
                  {category.features.map((feature) => (
                    <span class="text-sm px-3 py-1 bg-cyan-100 text-cyan-700 rounded-full">{feature}</span>
                  ))}
                </div>
              </div>
              <div class="mt-6 flex items-center text-primary-600 text-sm font-medium">
                Explore Technology
                <svg class="w-4 h-4 ml-1 group-hover:translate-x-1 transition-transform" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7"/>
                </svg>
              </div>
            </a>
          ))}
        </div>
      </div>
    </div>
  </section>

  <!-- Technology by Location -->
  <section class="py-16 bg-white">
    <div class="container">
      <div class="max-w-6xl mx-auto">
        <div class="text-center mb-12">
          <h2 class="text-3xl font-bold mb-4">Technology by <span class="gradient-text">Swiss Region</span></h2>
          <p class="text-gray-600 max-w-2xl mx-auto">
            Regional technology ecosystems and specialized digital innovation hubs
          </p>
        </div>

        <div class="grid md:grid-cols-2 lg:grid-cols-4 gap-6">
          {locationTechnology.map((location) => (
            <a href={location.href} class="bg-gradient-to-br from-gray-50 to-white rounded-2xl p-6 border border-gray-100 hover:shadow-lg transition-all duration-300 group hover:-translate-y-1">
              <h3 class="font-bold text-xl mb-3 group-hover:text-primary-600 transition-colors">{location.location}</h3>
              <p class="text-gray-600 text-sm mb-4 leading-relaxed">{location.description}</p>
              <div class="space-y-2">
                <div class="text-xs font-medium text-gray-500">Tech Focus:</div>
                <div class="flex flex-wrap gap-1">
                  {location.techFocus.map((focus) => (
                    <span class="text-xs px-2 py-1 bg-cyan-100 text-cyan-700 rounded-full">{focus}</span>
                  ))}
                </div>
              </div>
              <div class="mt-4 flex items-center text-primary-600 text-sm font-medium">
                Tech Ecosystem
                <svg class="w-4 h-4 ml-1 group-hover:translate-x-1 transition-transform" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7"/>
                </svg>
              </div>
            </a>
          ))}
        </div>
      </div>
    </div>
  </section>

  <!-- How Technology Solutions Work -->
  <section class="py-16 bg-gray-50">
    <div class="container">
      <div class="max-w-4xl mx-auto">
        <div class="text-center mb-12">
          <h2 class="text-3xl font-bold mb-4">How Technology Solutions Work</h2>
          <p class="text-gray-600 max-w-2xl mx-auto">
            Our systematic approach integrates cutting-edge technology throughout your relocation journey
          </p>
        </div>

        <div class="space-y-8">
          {serviceData.process?.map((step, index) => (
            <div class="flex items-start space-x-6 group">
              <div class="flex-shrink-0 w-12 h-12 bg-gradient-to-br from-cyan-500 to-cyan-600 rounded-xl flex items-center justify-center text-white font-bold text-lg group-hover:scale-110 transition-transform duration-300">
                {step.step}
              </div>
              <div class="flex-1">
                <div class="flex items-center justify-between mb-2">
                  <h3 class="font-bold text-xl">{step.title}</h3>
                  <span class="text-sm text-cyan-600 font-medium">{step.duration}</span>
                </div>
                <p class="text-gray-600 leading-relaxed">{step.description}</p>
              </div>
            </div>
          ))}
        </div>
      </div>
    </div>
  </section>

  <!-- Featured Technology Providers -->
  <section class="py-16 bg-white">
    <div class="container">
      <div class="max-w-6xl mx-auto">
        <div class="text-center mb-12">
          <h2 class="text-3xl font-bold mb-4">Featured <span class="gradient-text">Technology Providers</span></h2>
          <p class="text-gray-600 max-w-2xl mx-auto">
            Connect with innovative technology companies revolutionizing Swiss relocations
          </p>
        </div>

        <div class="grid md:grid-cols-2 gap-8">
          {companies.map((company) => (
            <CompanyCard company={company} />
          ))}
        </div>

        <div class="text-center mt-12">
          <a href="/companies?service=technology-solutions" class="btn-primary px-8 py-4">
            View All Technology Providers
          </a>
        </div>
      </div>
    </div>
  </section>

  <!-- Essential Reading Section -->
  <section class="py-16 bg-gray-50">
    <div class="container">
      <div class="max-w-6xl mx-auto">
        <div class="text-center mb-12">
          <h2 class="text-3xl font-bold mb-4">Essential <span class="gradient-text">Technology Resources</span></h2>
          <p class="text-gray-600 max-w-2xl mx-auto">
            Expert insights on digital innovation and technology trends in Swiss relocations
          </p>
        </div>

        <div class="grid md:grid-cols-2 lg:grid-cols-4 gap-6">
          {blogResources.map((resource) => (
            <a href={resource.href} class="bg-white rounded-2xl p-6 border border-gray-100 hover:shadow-lg transition-all duration-300 group">
              <div class="mb-4">
                <span class="text-xs font-medium px-3 py-1 bg-cyan-100 text-cyan-700 rounded-full">{resource.category}</span>
              </div>
              <h3 class="text-lg font-bold text-gray-900 mb-3 group-hover:text-primary-600 transition-colors">
                {resource.title}
              </h3>
              <div class="flex items-center justify-between text-xs text-gray-500">
                <span>{resource.readTime}</span>
                <span class="inline-flex items-center text-primary-600 hover:text-primary-700 font-medium">
                  Read More
                  <svg class="w-3 h-3 ml-1" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7"/>
                  </svg>
                </span>
              </div>
            </a>
          ))}
        </div>
      </div>
    </div>
  </section>

  <!-- Next Steps CTA -->
  <section class="py-16 bg-gradient-to-br from-cyan-600 to-blue-700 text-white">
    <div class="container">
      <div class="max-w-4xl mx-auto text-center">
        <h2 class="text-3xl md:text-4xl font-bold mb-6">
          Ready for Digital Relocation?
        </h2>
        <p class="text-xl text-white/90 mb-8">
          Experience the future of relocation with cutting-edge technology solutions designed for Swiss moves.
        </p>
        <div class="flex flex-wrap gap-4 justify-center">
                          <button onclick="openModal({page: 'service', service: 'Technology Solutions'})" class="bg-white text-cyan-600 px-8 py-4 rounded-xl font-semibold hover:bg-gray-100 transition-colors inline-flex items-center">
            Get Tech Solutions
            <svg class="w-5 h-5 ml-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M14 5l7 7-7 7"/>
            </svg>
          </button>
          <a href="/services/advisory-services" class="bg-white/10 backdrop-blur-sm border-2 border-white/30 text-white px-8 py-4 rounded-xl font-semibold hover:bg-white/20 transition-colors inline-flex items-center">
            Strategic Planning
            <svg class="w-5 h-5 ml-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9.663 17h4.673M12 3v1m6.364 1.636l-.707.707M21 12h-1M4 12H3m3.343-5.657l-.707-.707m2.828 9.9a5 5 0 117.072 0l-.548.547A3.374 3.374 0 0014 18.469V19a2 2 0 11-4 0v-.531c0-.895-.356-1.754-.988-2.386l-.548-.547z"/>
            </svg>
          </a>
        </div>
      </div>
    </div>
  </section>
</Layout> 